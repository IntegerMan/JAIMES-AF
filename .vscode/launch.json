{
    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET Core Launch (AppHost)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/JAIMES AF.AppHost/bin/Debug/net10.0/JAIMES AF.AppHost.dll",
            "args": [],
            "cwd": "${workspaceFolder}/JAIMES AF.AppHost",
            "stopAtEntry": false,
            "console": "integratedTerminal",
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development",
                "DOTNET_ENVIRONMENT": "Development",
                "ASPNETCORE_URLS": "http://localhost:15000",
                "ASPIRE_ALLOW_UNSECURED_TRANSPORT": "true",
                "ASPIRE_DASHBOARD_OTLP_ENDPOINT_URL": "http://localhost:19287",
                "ASPIRE_DASHBOARD_OTLP_HTTP_ENDPOINT_URL": "http://localhost:19287",
                "ASPIRE_RESOURCE_SERVICE_ENDPOINT_URL": "http://localhost:20027"
            },
            "serverReadyAction": {
                "action": "openExternally",
                "pattern": "Login to the dashboard at (https?://\\S+)",
                "uriFormat": "%s",
                "killOnServerStop": true
            }
        }
    ]
}