{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "DocumentChunking": {
    "ChunkingStrategy": "SemanticSlicer",
    "TokenLimit": 1024,
    "BufferSize": 2,
    "ThresholdType": "Percentile",
    "ThresholdAmount": 90.0,
    "TargetChunkCount": null,
    "MinChunkChars": 100,
    "SemanticSlicerMaxChunkTokenCount": 1000,
    "SemanticSlicerSeparators": "Text",
    "SemanticSlicerStripHtml": false
  }
}

